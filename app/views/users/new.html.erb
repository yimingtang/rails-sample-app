<% provide(:title, 'Sign Up') %>

<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <h2 class="form-header">Sign up</h2>
    <%= form_for @user, html: { role: "form" } do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "SIGN UP", class: "btn btn-lg btn-primary btn-block" %>
    <% end %>
  </div>
</div>
